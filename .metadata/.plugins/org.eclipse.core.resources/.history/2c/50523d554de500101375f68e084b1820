package iFexample;

import java.util.Scanner;

public class ifEx05 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.println("금액을 입력하세요");
		
		int price = scan.nextInt();
		
		if(price%50000 == 0) {
			System.out.println("오만원권: "+price/50000 +"매");
		}

	}

}
